{
  "name": "bike-market",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "shell:build": "npm run build -- --project shell",
    "shell:watch": "npm run watch -- --project shell",
    "shell:start": "npm run start -- --project shell",
    "catalog:start": "npm run start -- --project catalog",
    "catalog:build": "npm run build -- --project catalog",
    "catalog:watch": "npm run watch -- --project catalog",
    "catalog:test": "npm run test -- --project catalog",
    "recommendations:start": "npm run start -- --project recommendations",
    "recommendations:build": "npm run build -- --project recommendations",
    "recommendations:watch": "npm run watch -- --project recommendations",
    "recommendations:test": "npm run test -- --project recommendations",
    "chat:start": "npm run start -- --project chat",
    "chat:build": "npm run build -- --project chat",
    "chat:watch": "npm run watch -- --project chat",
    "chat:test": "npm run test -- --project chat",
    "models:build": "tsc --project projects/models/tsconfig.json",
    "payment:build": "npm --prefix ./projects/payment run build",
    "payment:watch": "npm --prefix ./projects/payment run watch",
    "payment:start": "npm --prefix ./projects/payment run dev",
    "cart:start": "npm run start -- --project cart",
    "cart:build": "npm run build -- --project cart",
    "cart:watch": "npm run watch -- --project cart",
    "cart:test": "npm run test -- --project cart",
    "chat-backend:build": "npm --prefix ./projects/chat-backend run build",
    "chat-backend:watch": "npm --prefix ./projects/chat-backend run watch",
    "chat-backend:start": "npm --prefix ./projects/chat-backend run start",
    "market-backend:build": "npm --prefix ./projects/market-backend run build",
    "market-backend:watch": "npm --prefix ./projects/market-backend run watch",
    "market-backend:start": "npm --prefix ./projects/market-backend run start",
    "all:start": "concurrently --kill-others \"npm run shell:start\" \"npm run catalog:start\" \"npm run recommendations:start\" \"npm run chat:start\" \"npm run payment:start\" \"npm run cart:start\" \"npm run chat-backend:build && npm run chat-backend:start\" \"npm run market-backend:build && npm run market-backend:start\"",
    "all:build": "npm run shell:build && npm run catalog:build && npm run recommendations:build && npm run chat:build && npm run payment:build && npm run cart:build && npm run chat-backend:build && npm run market-backend:build"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.2",
  "dependencies": {
    "@ag-ui/client": "^0.0.41",
    "@angular-architects/native-federation": "^21.0.0",
    "@angular/animations": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@softarc/native-federation": "^2.0.4",
    "@softarc/native-federation-runtime": "^2.0.4",
    "@tailwindcss/vite": "^4.1.18",
    "es-module-shims": "^1.5.12",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.3",
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.1.0",
    "concurrently": "^9.2.1",
    "esbuild": "^0.15.5",
    "jsdom": "^27.1.0",
    "json5": "^2.2.1",
    "ng-packagr": "^21.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "vitest": "^4.0.15"
  }
}
